body{background-color:#fff;font-size:12px}.oa-form-note h6{color:#050002;font-weight:700;font-size:12px}.oa-form-note .note-list{width:682px;margin:32px auto 0;padding-left:32px}.oa-form-note .note-list .note-list-ul{color:#050002;background-color:#f2f2f2;margin-top:11px}.oa-form-note .note-list .note-list-ul ul{border-bottom:1px solid #dedede;border-top:1px solid #dedede}.oa-form-note .note-list .note-list-ul ul li{border:1px solid #dedede}.oa-form-note .note-list .note-list-ul ul li .li-header{height:32px;line-height:32px;padding:0 18px;position:relative;cursor:pointer}.oa-form-note .note-list .note-list-ul ul li .li-header .note-tip{border-radius:4px;-webkit-border-radius:4px;width:29px;height:29px;position:absolute;left:-33px;top:0;background:url(../images/note-icon.png) 6px 1px no-repeat #8ec9ec}.oa-form-note .note-list .note-list-ul ul li .li-header .note-other{background-position:6px -48px}.oa-form-note .note-list .note-list-ul ul li .li-header .note-my{background-position:6px -110px}.oa-form-note .note-list .note-list-ul ul li .on{font-weight:700}.oa-form-note .note-list .note-list-ul ul li .li-content{border-top:1px solid #dedede;padding:20px;line-height:18px;overflow:hidden;display:none;word-wrap:break-word}.oa-form-note .note-list .note-list-ul ul .li-other .li-header .note-icon{background-position:6px -48px}.oa-form-note .note-list .note-list-ul ul .li-myself .li-header .note-icon{background-position:6px -110px}.oa-form-note .note-list .note-list-ul ul .li-open .li-header{font-weight:400}.oa-form-note .note-list .note-list-ul ul .li-open .li-content{display:block}.oa-form-note .note-list-none{margin:49px auto 0;background-color:#f2f2f2;width:682px;height:86px;border:1px solid #dedede;line-height:84px;text-align:center;color:#929292;font-weight:700}.oa-form-note .note-send{width:682px;margin:16px auto 0}.oa-form-note .note-send .textarea{width:100%;height:145px;border:1px solid #71b9ed;background-color:#f2f2f2;padding-top:10px;margin-top:8px}.oa-form-note .note-send .textarea textarea{border:0;background-color:#f2f2f2;width:95%;height:130px}.oa-form-note .note-send .send-btn{cursor:pointer;border:1px solid #4786d5;background:#5b97e1;border-radius:4px;-webkit-border-radius:4px;-moz-border-radius:4px;font-size:12px;padding:5px 24px;margin:17px auto 0;display:inline-block;-moz-transition:all 1s cubic-bezier(.175,.885,.32,1) 0s;-webkit-transition:all 1s cubic-bezier(.175,.885,.32,1) 0s;box-shadow:0 2px 2px #c5c5c5,2px 0 2px #c5c5c5;color:#fff;-webkit-box-shadow:0 2px 2px #c5c5c5,2px 0 2px #c5c5c5}.oa-form-note .note-send .send-btn:hover{color:#fff;border-color:#3bd;background-color:#3bd}.oa-form-note .note-send-has{padding-left:32px}