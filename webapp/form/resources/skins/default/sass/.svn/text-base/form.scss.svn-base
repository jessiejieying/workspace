/* 页签控件 tab */
.dynamic-tab-pane-control .tab-pane {
  position: relative;
  width: 100%; /* width needed weird IE bug */
  margin-right: -2px; /* to make room for the shadow */
}

.dynamic-tab-pane-control .tab-row .tab {
  width: 90px;
  height: 25px;
  background: #9D9D9D;
  position: relative;
  top: 0;
  display: inline;
  float: left;
  overflow: hidden;
  cursor: pointer;
  margin: 1px 1px 1px 2px;
  border: 0;
  color: #fff;
  z-index: 1;
  font-size: 12px;
  white-space: nowrap;
  text-align: center;
}

.dynamic-tab-pane-control .tab-row .tab a {
  font-size: 12px;
  color: #fff;
  text-decoration: none;
  line-height: 25px;
  display: block;
}

.dynamic-tab-pane-control .tab-row .tab a:hover {
  background: #D7D7D7;
}

.dynamic-tab-pane-control .tab-row .tab.selected {
  background: #64A9D7;
  z-index: 3;
}

.dynamic-tab-pane-control .tab-row .tab.selected a {
  cursor: default;
  background: #64A9D7;
}

.dynamic-tab-pane-control .tab-row .tab.selected a:hover {
  background: #64A9D7;
}

.dynamic-tab-pane-control .tab-page {
  clear: both;
  border: 1px solid #64A9D7;
  background: #fff;
  z-index: 2;
  position: relative;
  top: -2px;
  font-size: 12px;
  color: Black;
  padding: 0px;
}

.dynamic-tab-pane-control .tab-row {
  z-index: 1;
  white-space: nowrap;
}

