//查看传阅 表格

table.oa-read-pass-grid {
  width: 100% !important;
  margin-right: 5px;
  table-layout: fixed;
  border-collapse: collapse;

  .pass-grid-intable {
    width: 595px;
    td {
      height: 38px;
    }
  }
}

.oa-read-pass-grid {
  font-size: 12px;
//  font-family: '宋体', '微软雅黑', Verdana, sans-serif, serif;
  border: 2px solid #71b8ec;
  margin-top: 60px;

  th, td {
    border-bottom: 1px solid #98cef4;
    border-right: 1px solid #98cef4;
    background-color: white;
    text-align: left !important;
    padding-left: 14px;
    color: #333;
    height: 38px;
    vertical-align: middle;

  }
  td {

    .lineWrap {
      word-break: break-all;

    }
    .lineNoWrap {
      white-space: nowrap;
      overflow: hidden;
      text-overflow: ellipsis
    }

  }
  th {
    font-weight: 400;
  }

  tr {
    .even_index_row td {
      background-color: #eef
    }
    .row_hover td {
      background-color: #fff1cc !important
    }
    .selected.selected_color td {
      background-color: #dfe8f6 !important
    }

    &:hover td {
      background-color: #dfe8f6 !important;
    }
  }
  .interlaced-th td {
    background-color: #f2f2f2;
  }
  a {
    color: #f60;
    text-decoration: none
  }
  .lockscreen {
    position: fixed;
    z-index: 2014;
    display: none;
    right: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: #000;
    filter: alpha(opacity=10);
    opacity: .1;
    -moz-opacity: .1
  }
  .loading_div {
    position: fixed;
    z-index: 2015;
    display: none;
    right: 0;
    top: 0;
    width: 100%;
    height: 100%
  }
}

