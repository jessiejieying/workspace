	}
	
	var url = "../../fceform/common/djframe.htm?djsn=wf_eform_url_list&djtype=WF_DSN";
	var arr = window.showModalDialog(url, window, "dialogHeight:580px; dialogWidth:900px;help:no;center:yes;status:no;resizable:no;location:yes;scroll:no");
	if (arr==null) return;

	$id("txtView").value = arr[0];
	$id("txtView").title = arr[1];
}
var accessControls ="";
var stepOptions="";//2012-4-1
function uf_onload(){
        
       //putChildFrameDoc()
  

	//SetButtonImage(FCButton5,"../../fceform/images/ef_run_button_ok.gif");
	//SetButtonImage(FCButton6,"../../fceform/images/ef_run_button_close.gif");
	PropWinOnload();

	dropdownlist9.FieldNameList="task_type_chn,task_type";
	
	//属性页
	var objOpener = window.dialogArguments;
	var objNodePropertys = objOpener.G_NodePropertys;
	var objAction  = new NodePropertys();
	if( objNodePropertys ){
	
		$id("txtID").value = objNodePropertys.ID;
		$id("txtName").value = objNodePropertys.Name;
		if (objNodePropertys.Compensate)
			$id("txtComClass").value = objNodePropertys.Compensate;
		
		//增加动作的属性设置 
		$id("txtActionID").value = objNodePropertys.ActionID;
		if (objNodePropertys.ActionProperty){
			objAction = objNodePropertys.ActionProperty;
	
			//$id("txtID").value = objNodePropertys.ID;
			//$id("txtName").value = objNodePropertys.Name;			
			if (objAction.View)
				$id("txtView").value = objAction.View;
				
			if (objAction.Remark)
				$id("txtRemark").value = objAction.Remark;
								
			if (objAction.Auto && objAction.Auto=="true")
				SetCheckBoxValue(chkAuto,"true");	
				
			if (objAction.Finish && objAction.Finish =="true")
				SetCheckBoxValue(chkFinish,"true");	
						
			if (objAction.UserActionType && !IsSpace(objAction.UserActionType))
				$id("cboActionType").value = objAction.UserActionType;
												
			if (objAction.bs_table)
				$id("bsTableName").value = objAction.bs_table;
			if (objAction.bs_id_field)	
				$id("bsIdFieldName").value = objAction.bs_id_field;
			if (objAction.bs_desc_field)
				$id("bsDescFieldName").value = objAction.bs_desc_field ;
			//业务表单关联动态分支实例ID
			if (objAction.rela_instance_id && objAction.rela_instance_id=="true")
				SetCheckBoxValue(chkRelaInstanceId,"true");					
			//增加access-controls 表权限设置xml 2011-7-15
			if (typeof(objAction.accesscontrol)!="undefined"&& objAction.accesscontrol.length>0)
				accessControls = objAction.accesscontrol;

		}
		
	}


	//得到当前步骤下动作下拉项
	//var Nodes = objOpener.FlowArea.FlowNodes.Nodes;
	var sActionHtml = "";	
	//复合节点中，一个步骤只能带一个动作 去掉这个查询
	/*
	for (var i=0;i<Nodes.length;i++){	
		if (Nodes[i].FromNode && Nodes[i].FromNode==objNodePropertys.ID && Nodes[i].NodeType == "Result" ){
			for (var j=0;j<Nodes.length;j++){
				if (Nodes[j].id && Nodes[j].id == Nodes[i].ToNode && Nodes[j].NodeType=="ActionNode"){
					sActionHtml += "<tr heigth='16px'>";
					sActionHtml += "<td>" + Nodes[j].id +"</td>";
					sActionHtml += "<td>" + Nodes[j].Name + "</td>";
					sActionHtml += "</tr>";			
					break;					
				}
			}
		}			
	}*/
	
	//任务页
	loadActionCbo(cboTaskAction,sActionHtml);
	if ((typeof(objNodePropertys.task)!="undefined"&& objNodePropertys.task.length>0)){
		loadTaskInfo(objNodePropertys.task);
	}
	else{
		dsTask.Append();	
	}
	
	//得到步骤下拉项xml串
	var Nodes = objOpener.FlowArea.FlowNodes.Nodes;
	var sHtml = "<tr height='16px'><td></td><td></td></tr>";
	stepOptions = "<option value=''></option>";//准备给任务的用户参数用
	for (var i=0;i<Nodes.length;i++){					
		if (Nodes[i].id && (Nodes[i].NodeType == "StepNode" || Nodes[i].NodeType == "CompNode")){
			sHtml += "<tr height='16px'>";
			sHtml += "<td>" + Nodes[i].id +"</td>";
			sHtml += "<td>" + Nodes[i].Name + "</td>";
			sHtml += "</tr>";
			stepOptions +="<option value='" + Nodes[i].id + "'>" + Nodes[i].Name + "</option>";
		}			
	}
	

	//条件页  修改成动作的条件，去掉步骤的条件设置 2011-3-8
	loadStepCbo(cboCondStep,sHtml);
	if ((typeof(objAction.permission)!="undefined"&& objAction.permission.length>0)){
		loadCondInfo(objAction.permission);
	}else{
		dsCond.Append();
	}
	
	//前置函数页
	loadStepCbo(cboPrefStep,sHtml);
	var stepPrefXml ="";
	var actionPrefXml ="";
	if (typeof(objNodePropertys.prefunction)!="undefined"&& objNodePropertys.prefunction.length>0)
		stepPrefXml = objNodePropertys.prefunction;	
	if (typeof(objAction.prefunction)!="undefined"&& objAction.prefunction.length>0)
		actionPrefXml = objAction.prefunction;
	
	if (stepPrefXml.length>0 || actionPrefXml.length>0)	
		loadPrefInfo(stepPrefXml,actionPrefXml);
	
	else{
		dsPref.Append();	
	}	
	
	//后置函数页
	loadStepCbo(cboPostStep,sHtml);
	var stepPostXml = "",actionPostXml = "";
	if (typeof(objNodePropertys.postfunction)!="undefined"&& objNodePropertys.postfunction.length>0)
		stepPostXml = objNodePropertys.postfunction;
	if (typeof(objAction.postfunction)!="undefined"&& objAction.postfunction.length>0)
		actionPostXml = objAction.postfunction;
	if (stepPostXml.length>0 || actionPostXml.length>0)	{	
		loadPostInfo(stepPostXml,actionPostXml);
	}
	else{
		dsPost.Append();
	}

	//设置表权限权限 按钮是否显示
	if (AccessControlPage!="")
		$id("cmdAccessControl").style.display = "block";

}


function loadCondInfo(permXml){
	var oXml = SetDom(permXml);	
	var oList = oXml.getElementsByTagName("conditions");
	if (oList.length>0)
		$id("cboPermType").value = oList.item(0).getAttribute("type");
			
	oList = oXml.getElementsByTagName("condition");
	var len = oList.length;
	//给dsCond赋值
	var strXml = LoadXmlToDataset("Cond",oList);
	var ss = dsCond.xml;
	ss = ss.substring(6,ss.length);
	//CopyToPub(dsCond.xml);

	ss= "<root>"+strXml+ss;
	dsCond.OpenXmlData(ss);	

		
}
function loadPrefInfo(stepPrefXml,actionPrefXml){
	var oXml = SetDom(stepPrefXml);	
	var oListStep = oXml.getElementsByTagName("function");
	oXml = SetDom(actionPrefXml);
	var oListAction = oXml.getElementsByTagName("function");
	//给dsPref赋值
	var strXml = LoadXmlToDatasetForComp("Pref",oListStep,oListAction);
	//CopyToPub(dsPref.xml);
	var ss = dsPref.xml;
	ss = ss.substring(6,ss.length);
	
	ss= "<root>"+strXml+ss;
	dsPref.OpenXmlData(ss);			
}
function loadPostInfo(stepPostXml,actionPostXml){
	var oXml = SetDom(stepPostXml);	
	var oListStep = oXml.getElementsByTagName("function");
	oXml = SetDom(actionPostXml);
	var oListAction = oXml.getElementsByTagName("function");
	//给dsPref赋值
	var strXml = LoadXmlToDatasetForComp("Post",oListStep,oListAction);
	//CopyToPub(dsPost.xml);
	var ss = dsPost.xml;
	ss = ss.substring(6,ss.length);

	ss= "<root>"+strXml+ss;
	dsPost.OpenXmlData(ss);			
}

function loadTaskInfo(taskXml){
	//任务标签页
   /*<task name="部门主管审核" classname="cn.com.fcsoft.workflow.util.MakeTask">
      <arg name="taskDesc">审核员工请假申请</arg>
      <arg name="stepId">802</arg>
      <arg name="rolename">部门经理</arg>
      <arg name="role">ROL_00000003</arg>
      <arg name="groupname"></arg>
      <arg name="group"></arg>
      <arg name="username"></arg>
      <arg name="user"></arg>
    </task>		*/

	var oXml = SetDom(taskXml);	
	var oList = oXml.getElementsByTagName("task");
	var len = oList.length;	
	//给dsTask赋记录值
	var strXml = "";	
	for (var i=0;i<len;i++){
		strXml +="<tr>";
		strXml +="<td>" + oList.item(i).getAttribute("name") + "</td>";//任务名称
		//var taskDesc = "";	
		var taskType="",taskTypeChn="",actionId="",actionName="",signScale="",finishDays="";
		var roleids="",rolenames="",groupids="",groupnames="",userids="",usernames="";
		var copyRoleids="",copyRolenames="",copyGroupids="",copyGroupnames="",copyUserids="",copyUsernames="";
		
		for (var j=0;j<oList.item(i).childNodes.length;j++){
			var attrname = oList.item(i).childNodes(j).getAttribute("name");
			if (attrname=="taskType") taskType = oList.item(i).childNodes(j).text;
			if (attrname=="signScale") signScale = oList.item(i).childNodes(j).text;
			if (attrname=="finishDays") finishDays = oList.item(i).childNodes(j).text;
			
			if (attrname=="taskTypeChn") taskTypeChn = oList.item(i).childNodes(j).text;
			if (attrname=="actionId") actionId = oList.item(i).childNodes(j).text;
			if (attrname=="actionName") actionName = oList.item(i).childNodes(j).text;
			//if (attrname=="useActionConArg") useActionConArg = oList.item(i).childNodes(j).text;
			
			
			//if (attrname=="taskDesc") taskDesc = oList.item(i).childNodes(j).text;
			if (attrname=="role") roleids = oList.item(i).childNodes(j).text;
			if (attrname=="rolename") rolenames = oList.item(i).childNodes(j).text;
			if (attrname=="copy_role") copyRoleids = oList.item(i).childNodes(j).text;
			if (attrname=="copy_rolename") copyRolenames = oList.item(i).childNodes(j).text;
			
			if (attrname=="group") groupids = oList.item(i).childNodes(j).text;
			if (attrname=="groupname") groupnames = oList.item(i).childNodes(j).text;
			if (attrname=="copy_group") copyGroupids = oList.item(i).childNodes(j).text;
			if (attrname=="copy_groupname") copyGroupnames = oList.item(i).childNodes(j).text;

			if (attrname=="user") userids = oList.item(i).childNodes(j).text;
			if (attrname =="username") usernames = oList.item(i).childNodes(j).text;
			if (attrname=="copy_user") copyUserids = oList.item(i).childNodes(j).text;
			if (attrname =="copy_username") copyUsernames = oList.item(i).childNodes(j).text;

		}

		var varclasses="";//增加2012-4-1
		var oClass = oList.item(i).getElementsByTagName("var-class");
		for (var ix=0;ix<oClass.length;ix++){
			varclasses += oClass.item(ix).xml;	
		}		
		
		var reminder ="";//增加reminder2012-5-14
		var oReminder = oList.item(i).getElementsByTagName("reminder");
		
		for (var ii=0;ii<oReminder.length;ii++){
			reminder  += oReminder.item(ii).xml;	
		} 
		//strXml +="<td>" + taskDesc + "</td>";//任务描述
		strXml +="<td>" + taskType + "</td>";
		strXml +="<td>" + taskTypeChn + "</td>";
		strXml +="<td>" + actionId + "</td>";
		strXml +="<td>" + actionName + "</td>";
		//strXml +="<td>" + useActionConArg + "</td>";
		
		strXml +="<td>" + userids + "</td>";
		strXml +="<td>" + usernames + "</td>";
		strXml +="<td>" + copyUserids + "</td>";
		strXml +="<td>" + copyUsernames + "</td>";
				
		strXml +="<td>" + roleids + "</td>";
		strXml +="<td>" + rolenames + "</td>";
		strXml +="<td>" + copyRoleids + "</td>";
		strXml +="<td>" + copyRolenames + "</td>";
				
		strXml +="<td>" + groupids + "</td>";
		strXml +="<td>" + groupnames + "</td>";
		strXml +="<td>" + copyGroupids + "</td>";
		strXml +="<td>" + copyGroupnames + "</td>";		
		
		strXml +="<td>" + signScale + "</td>"; //会签百分比
		strXml +="<td>" + finishDays + "</td>"; //预计完成天数
		
		strXml +="<td>" + escape(varclasses) + "</td>"; //变量类
		
		strXml +="<td>" + escape(reminder) + "</td>"; //提醒
		strXml +="</tr>";
	}
	//CopyToPub(dsTask.xml);
	
	var ss = dsTask.xml;
	var pos = ss.indexOf("<set><pages>");
	if (pos>=0){
		ss = ss.substring(pos,ss.length);	
	}else{
		ss = ss.substring(6,ss.length);
	}	
	
	ss= "<root>"+strXml+ss;
	dsTask.OpenXmlData(ss);	
	
	//alert(divTaskUser.innerHTML);
}



/*
数据集dsTask上滚动事件，滚动时候修改divUser,divRole,divGroup中的显示
*/
function uf_task_scroll(){
	//获取divTaskUser等的显示；
	var userids = dsTask.Field("task_user_id").Value;
	var usernames = dsTask.Field("task_user").Value;
	var copyUserids = dsTask.Field("task_copy_user_id").Value;
	var copyUsernames = dsTask.Field("task_copy_user").Value;
	
	var roleids = dsTask.Field("task_role_id").Value;
	var rolenames = dsTask.Field("task_role").Value;
	var copyRoleids = dsTask.Field("task_copy_role_id").Value;
	var copyRolenames = dsTask.Field("task_copy_role").Value;
		
	var groupids = dsTask.Field("task_group_id").Value;
	var groupnames = dsTask.Field("task_group").Value;		
	var copyGroupids = dsTask.Field("task_copy_group_id").Value;
	var copyGroupnames = dsTask.Field("task_copy_group").Value;		
	
	//2012-4-1加
	//清空div中的显示
	divTaskUser.innerHTML="";
	divTaskRole.innerHTML="";
	divTaskGroup.innerHTML="";
	
	var varclasses = dsTask.Field("var_class").Value;
	if (userids.length>0) showDivInfo("divTaskUser",userids,usernames);
	if (copyUserids.length>0) showDivInfo("divTaskUser",copyUserids,copyUsernames,"true"); 
	if (roleids.length>0) showDivInfo("divTaskRole",roleids,rolenames); 
	if (copyRoleids.length>0) showDivInfo("divTaskRole",copyRoleids,copyRolenames,"true"); 
	if (groupids.length>0) showDivInfo("divTaskGroup",groupids,groupnames);
	if (copyGroupids.length>0) showDivInfo("divTaskGroup",copyGroupids,copyGroupnames,"true");	 
	
	//显示 变量类2012-4-1
	if (varclasses!=""){
		var oClass = SetDom("<var-classes>" + unescape(varclasses) + "</var-classes>");
		showVarClassDivInfo(oClass);				
	}
	
	uf_typechange();	
}



//页签控件的点击事件
function PubClickPageControl(index) {
	if(index == 1){
		gridTask.ParentPos = "相对" ;
		gridTask.hideVscroll = false ;
		gridTask.hideHscroll = false ;
		gridTask.HScroll();  //重算滚动条
		gridTask.VScroll();
	}
	if(index == 2){
		gridCond.ParentPos = "相对" ;
		gridCond.hideVscroll = false ;
		gridCond.hideHscroll = false ;
		gridCond.HScroll();  //重算滚动条
		gridCond.VScroll();
	}
	if(index == 3){
		gridPref.ParentPos = "相对" ;
		gridPref.hideVscroll = false ;
		gridPref.hideHscroll = false ;
		gridPref.HScroll();  //重算滚动条
		gridPref.VScroll();
	}	
	if(index == 4){
		gridPost.ParentPos = "相对" ;
		gridPost.hideVscroll = false ;
		gridPost.hideHscroll = false ;
		gridPost.HScroll();  //重算滚动条
		gridPost.VScroll();
	}		
}



function uf_return(){
	var objProperty = new NodePropertys();
	objProperty.ID = $id("txtID").value;
	objProperty.Name =$id("txtName").value;
		
	objProperty.Compensate = $id("txtComClass").value; //业务补偿类名
	
	//动作的属性对象
	objProperty.ActionID = $id("txtActionID").value;
	var objAction = new NodePropertys();
	objAction.ID = objProperty.ActionID;
	objAction.Name = objProperty.Name;
	
	objAction.View = $id("txtView").value; //url
	objAction.Remark = $id("txtRemark").value;
	
	if ($id("chkAuto").value=="true")
		objAction.Auto = "true";
	else
		objAction.Auto = "";
	if ($id("chkFinish").value=="true")
		objAction.Finish = "true";
	else
		objAction.Finish = "";	

	if (!IsSpace($id("cboActionType").value))
		objAction.UserActionType = $id("cboActionType").value;
	else
		objAction.UserActionType = "";
	
	objAction.bs_table = $id("bsTableName").value;
	objAction.bs_id_field = $id("bsIdFieldName").value;
	objAction.bs_desc_field = $id("bsDescFieldName").value;		
	if ($id("chkRelaInstanceId").value=="true")
		objAction.rela_instance_id = "true";
	else
		objAction.rela_instance_id = "";	

	if(!IsSpace(accessControls))
		objAction.accesscontrol = accessControls;
	
	//任务标签页
   /*<task name="部门主管审核" classname="cn.com.fcsoft.workflow.util.MakeTask">
      <arg name="taskDesc">审核员工请假申请</arg>
      <arg name="stepId">802</arg>
      <arg name="rolename">部门经理</arg>
      <arg name="role">ROL_00000003</arg>
      <arg name="groupname"></arg>
      <arg name="group"></arg>
      <arg name="username"></arg>
      <arg name="user"></arg>
    </task>		*/
	var taskXml = "";
	if(dsTask.Update()==1) { 
		alert("任务设置有错，请修改后再确认！");
		return;
	}
	
	var oXml = dsTask.oDom;
	
	var len = oXml.documentElement.childNodes.length-1;
	for (var i=0;i<len;i++){
		//任务名称必须都填写了，才是有效的
		var taskname = oXml.documentElement.childNodes(i).childNodes(0).text;
		//判断任务名称是否填写
		var user = oXml.documentElement.childNodes(i).childNodes(5).text;
		var role = oXml.documentElement.childNodes(i).childNodes(9).text;
		var group = oXml.documentElement.childNodes(i).childNodes(13).text;
		if ((!IsSpace(user) || !IsSpace(role) || !IsSpace(group)) && IsSpace(taskname)){
			alert("任务描述必需要填写，才能完成任务信息的设置！");
			return;
		}
		
		if (!IsSpace(taskname)){
			taskXml += '<task name="'+ taskname + '" classname="com.rj.eden.wf.workflow.util.GenerateTask">';

			//taskXml += '<arg name="taskDesc">' + oXml.documentElement.childNodes(i).childNodes(1).text +'</arg>';
			taskXml += '<arg name="taskType">' + oXml.documentElement.childNodes(i).childNodes(1).text +'</arg>';
			taskXml += '<arg name="taskTypeChn">' + oXml.documentElement.childNodes(i).childNodes(2).text +'</arg>';
			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(3).text)){
				taskXml += '<arg name="actionId">' + oXml.documentElement.childNodes(i).childNodes(3).text +'</arg>';
				taskXml += '<arg name="actionName">' + oXml.documentElement.childNodes(i).childNodes(4).text +'</arg>';
			}	
			//if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(5).text))
				//taskXml += '<arg name="useActionConArg">' + oXml.documentElement.childNodes(i).childNodes(5).text +'</arg>';

			taskXml +='<arg name="stepId">' + $id("txtID").value +'</arg>';
			//user
			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(5).text))
				taskXml +='<arg name="user">' + oXml.documentElement.childNodes(i).childNodes(5).text + '</arg>';
			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(6).text))
				taskXml +='<arg name="username">' + oXml.documentElement.childNodes(i).childNodes(6).text + '</arg>';
			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(7).text))
				taskXml +='<arg name="copy_user">' + oXml.documentElement.childNodes(i).childNodes(7).text + '</arg>';
			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(8).text))
				taskXml +='<arg name="copy_username">' + oXml.documentElement.childNodes(i).childNodes(8).text + '</arg>';


			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(9).text))			
				taskXml +='<arg name="role">' + oXml.documentElement.childNodes(i).childNodes(9).text + '</arg>';
			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(10).text))
				taskXml +='<arg name="rolename">' + oXml.documentElement.childNodes(i).childNodes(10).text + '</arg>';
			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(11).text))			
				taskXml +='<arg name="copy_role">' + oXml.documentElement.childNodes(i).childNodes(11).text + '</arg>';
			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(12).text))
				taskXml +='<arg name="copy_rolename">' + oXml.documentElement.childNodes(i).childNodes(12).text + '</arg>';
			
			
			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(13).text))
				taskXml +='<arg name="group">' + oXml.documentElement.childNodes(i).childNodes(13).text + '</arg>';						
			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(14).text))
				taskXml +='<arg name="groupname">' + oXml.documentElement.childNodes(i).childNodes(14).text + '</arg>';
			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(15).text))
				taskXml +='<arg name="copy_group">' + oXml.documentElement.childNodes(i).childNodes(15).text + '</arg>';						
			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(16).text))
				taskXml +='<arg name="copy_groupname">' + oXml.documentElement.childNodes(i).childNodes(16).text + '</arg>';			
			
			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(17).text))
				taskXml += '<arg name="signScale">' + oXml.documentElement.childNodes(i).childNodes(17).text +'</arg>';
			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(18).text))
				taskXml += '<arg name="finishDays">' + oXml.documentElement.childNodes(i).childNodes(18).text +'</arg>';
				
			//2012-4-1
			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(19).text))
				taskXml += '<var-classes>' + unescape(oXml.documentElement.childNodes(i).childNodes(19).text) +'</var-classes>';
							
			//2012-5-14
			if (!IsSpace(oXml.documentElement.childNodes(i).childNodes(20).text))
				taskXml += unescape(oXml.documentElement.childNodes(i).childNodes(20).text);

			taskXml +='</task>';
		}			
	}
	if (taskXml.length>0){
		objProperty.task = "<tasks>" + taskXml + "</tasks>";
	}	
	//许可约束页
	/*<external-permissions>
			<permission name="permC">
				<restrict-to>
					<conditions type="AND">
						<condition type="class">
							<arg name="class.name">com.opensymphony.workflow.util.StatusCondition</arg>
							<arg name="status">Underway</arg>
						</condition>
						<condition type="class">
							<arg name="class.name">com.opensymphony.workflow.util.AllowOwnerOnlyCondition</arg>
						</condition>
					</conditions>
				</restrict-to>
			</permission>
	</external-permissions>*/
	
	//动作的条件
	if(dsCond.Update()==1) { 
		alert("约束条件设置有错，请修改后再确认！");
		return;
	}
	var oXml = dsCond.oDom;
	if (dsCond.oDom.documentElement.childNodes.length>2 && IsSpace($id("cboPermType").value) ){
		alert("请设置多个条件的 条件间关系！");
		//if ($id("cboPermType").style.display!="none")	$id("cboPermType").focus();
		return;
	}
	var permissionXml = SetDatasetToXml("condition",oXml);
	if (permissionXml.length>0){
		var sTemp ='<restrict-to><conditions type="'+ $id("cboPermType").value + '">'	;						
		permissionXml  = sTemp + permissionXml + "</conditions></restrict-to>";
					
		objAction.permission = permissionXml;
	}

	

	
	//前置函数页
	/*<pre-functions>
			<function type="class">
				<arg name="class.name">com.opensymphony.workflow.util.MostRecentOwner</arg>
				<arg name="stepId">3</arg>
			</function>
	</pre-functions>		*/

	//步骤 和 动作的 前置
	if (dsPref.Update()==1){
		alert("前置函数设置有错，请修改后再确认！");
		return;
	}
	var oXml = dsPref.oDom;
	
	var objReturn = SetDatasetToXmlForComp("function",oXml);	
	
	if(objReturn.stepXml.length>0){
		objReturn.stepXml  = "<pre-functions>" + objReturn.stepXml + "</pre-functions>";
		objProperty.prefunction = objReturn.stepXml		
	}
	
	if(objReturn.actionXml.length>0){
		objReturn.actionXml  = "<pre-functions>" + objReturn.actionXml + "</pre-functions>";
		objAction.prefunction = objReturn.actionXml	
	}
	//end
	
   //后置函数页
	/*<pre-functions>
			<function type="class">
				<arg name="class.name">com.opensymphony.workflow.util.MostRecentOwner</arg>
				<arg name="stepId">3</arg>
			</function>
	</pre-functions>		*/

	if (dsPost.Update()==1){
		alert("后置函数设置有错，请修改后再确认！");
		return;
	}
	var oXml = dsPost.oDom;
	var objReturn = SetDatasetToXmlForComp("function",oXml);	

	if (objReturn.stepXml.length>0){
		objReturn.stepXml  = "<post-functions>" + objReturn.stepXml + "</post-functions>";
		objProperty.postfunction = objReturn.stepXml
	}	
	if (objReturn.actionXml.length>0){
		objReturn.actionXml  = "<post-functions>" + objReturn.actionXml + "</post-functions>";
		objAction.postfunction = objReturn.actionXml
	}		
	//end
	
	objProperty.ActionProperty = objAction;
	window.returnValue = objProperty;
	window.close();	
}


