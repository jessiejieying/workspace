#10;			sHtml += "</tr>";&#13;&#10;			stepOptions +="<option value='" + Nodes[i].id + "'>" + Nodes[i].Name + "</option>";&#13;&#10;		}			&#13;&#10;	}&#13;&#10;	&#13;&#10;&#13;&#10;	//条件页  修改成动作的条件，去掉步骤的条件设置 2011-3-8&#13;&#10;	loadStepCbo(cboCondStep,sHtml);&#13;&#10;	if ((typeof(objAction.permission)!="undefined"&& objAction.permission.length>0)){&#13;&#10;		loadCondInfo(objAction.permission);&#13;&#10;	}else{&#13;&#10;		dsCond.Append();&#13;&#10;	}&#13;&#10;	&#13;&#10;	//前置函数页&#13;&#10;	loadStepCbo(cboPrefStep,sHtml);&#13;&#10;	var stepPrefXml ="";&#13;&#10;	var actionPrefXml ="";&#13;&#10;	if (typeof(objNodePropertys.prefunction)!="undefined"&& objNodePropertys.prefunction.length>0)&#13;&#10;		stepPrefXml = objNodePropertys.prefunction;	&#13;&#10;	if (typeof(objAction.prefunction)!="undefined"&& objAction.prefunction.length>0)&#13;&#10;		actionPrefXml = objAction.prefunction;&#13;&#10;	&#13;&#10;	if (stepPrefXml.length>0 || actionPrefXml.length>0)	&#13;&#10;		loadPrefInfo(stepPrefXml,actionPrefXml);&#13;&#10;	&#13;&#10;	else{&#13;&#10;		dsPref.Append();	&#13;&#10;	}	&#13;&#10;	&#13;&#10;	//后置函数页&#13;&#10;	loadStepCbo(cboPostStep,sHtml);&#13;&#10;	var stepPostXml = "",actionPostXml = "";&#13;&#10;	if (typeof(objNodePropertys.postfunction)!="undefined"&& objNodePropertys.postfunction.length>0)&#13;&#10;		stepPostXml = objNodePropertys.postfunction;&#13;&#10;	if (typeof(objAction.postfunction)!="undefined"&& objAction.postfunction.length>0)&#13;&#10;		actionPostXml = objAction.postfunction;&#13;&#10;	if (stepPostXml.length>0 || actionPostXml.length>0)	{	&#13;&#10;		loadPostInfo(stepPostXml,actionPostXml);&#13;&#10;	}&#13;&#10;	else{&#13;&#10;		dsPost.Append();&#13;&#10;	}&#13;&#10;&#13;&#10;	//设置表权限权限 按钮是否显示&#13;&#10;	if (AccessControlPage!="")&#13;&#10;		$id("cmdAccessControl").style.display = "block";&#13;&#10;&#13;&#10;}&#13;&#10;&#13;&#10;&#13;&#10;function loadCondInfo(permXml){&#13;&#10;	var oXml = SetDom(permXml);	&#13;&#10;	var oList = oXml.getElementsByTagName("conditions");&#13;&#10;	if (oList.length>0)&#13;&#10;		$id("cboPermType").value = oList.item(0).getAttribute("type");&#13;&#10;			&#13;&#10;	oList = oXml.getElementsByTagName("condition");&#13;&#10;	var len = oList.length;&#13;&#10;	//给dsCond赋值&#13;&#10;	var strXml = LoadXmlToDataset("Cond",oList);&#13;&#10;	var ss = dsCond.xml;&#13;&#10;	ss = ss.substring(6,ss.length);&#13;&#10;	//CopyToPub(dsCond.xml);&#13;&#10;&#13;&#10;	ss= "<root>"+strXml+ss;&#13;&#10;	dsCond.OpenXmlData(ss);	&#13;&#10;&#13;&#10;		&#13;&#10;}&#13;&#10;function loadPrefInfo(stepPrefXml,actionPrefXml){&#13;&#10;	var oXml = SetDom(stepPrefXml);	&#13;&#10;	var oListStep = oXml.getElementsByTagName("function");&#13;&#10;	oXml = SetDom(actionPrefXml);&#13;&#10;	var oListAction = oXml.getElementsByTagName("function");&#13;&#10;	//给dsPref赋值&#13;&#10;	var strXml = LoadXmlToDatasetForComp("Pref",oListStep,oListAction);&#13;&#10;	//CopyToPub(dsPref.xml);&#13;&#10;	var ss = dsPref.xml;&#13;&#10;	ss = ss.substring(6,ss.length);&#13;&#10;	&#13;&#10;	ss= "<root>"+strXml+ss;&#13;&#10;	dsPref.OpenXmlData(ss);			&#13;&#10;}&#13;&#10;function loadPostInfo(stepPostXml,actionPostXml){&#13;&#10;	var oXml = SetDom(stepPostXml);	&#13;&#10;	var oListStep = oXml.getElementsByTagName("function");&#13;&#10;	oXml = SetDom(actionPostXml);&#13;&#10;	var oListAction = oXml.getElementsByTagName("function");&#13;&#10;	//给dsPref赋值&#13;&#10;	var strXml = LoadXmlToDatasetForComp("Post",oListStep,oListAction);&#13;&#10;	//CopyToPub(dsPost.xml);&#13;&#10;	var ss = dsPost.xml;&#13;&#10;	ss = ss.substring(6,ss.length);&#13;&#10;&#13;&#10;	ss= "<root>"+strXml+ss;&#13;&#10;	dsPost.OpenXmlData(ss);			&#13;&#10;}&#13;&#10;&#13;&#10;function loadTaskInfo(taskXml){&#13;&#10;	//任务标签页&#13;&#10;   /*<task name="部门主管审核" classname="cn.com.fcsoft.workflow.util.MakeTask">&#13;&#10;      <arg name="taskDesc">审核员工请假申请</arg>&#13;&#10;      <arg name="stepId">802</arg>&#13;&#10;      <arg name="rolename">部门经理</arg>&#13;&#10;      <arg name="role">ROL_00000003</arg>&#13;&#10;      <arg name="groupname"></arg>&#13;&#10;      <arg name="group"></arg>&#13;&#10;      <arg name="username"></arg>&#13;&#10;      <arg name="user"></arg>&#13;&#10;    </task>		*/&#13;&#10;&#13;&#10;	var oXml = SetDom(taskXml);	&#13;&#10;	var oList = oXml.getElementsByTagName("task");&#13;&#10;	var len = oList.length;	&#13;&#10;	//给dsTask赋记录值&#13;&#10;	var strXml = "";	&#13;&#10;	for (var i=0;i<len;i++){&#13;&#10;		strXml +="<tr>";&#13;&#10;		strXml +="<td>" + oList.item(i).getAttribute("name") + "</td>";//任务名称&#13;&#10;		//var taskDesc = "";	&#13;&#10;		var taskType="",taskTypeChn="",actionId="",actionName="",signScale="",finishDays="";&#13;&#10;		var roleids="",rolenames="",groupids="",groupnames="",userids="",usernames="";&#13;&#10;		var copyRoleids="",copyRolenames="",copyGroupids="",copyGroupnames="",copyUserids="",copyUsernames="";&#13;&#10;		&#13;&#10;		for (var j=0;j<oList.item(i).childNodes.length;j++){&#13;&#10;			var attrname = oList.item(i).childNodes(j).getAttribute("name");&#13;&#10;			if (attrname=="taskType") taskType = oList.item(i).childNodes(j).text;&#13;&#10;			if (attrname=="signScale") signScale = oList.item(i).childNodes(j).text;&#13;&#10;			if (attrname=="finishDays") finishDays = oList.item(i).childNodes(j).text;&#13;&#10;			&#13;&#10;			if (attrname=="taskTypeChn") taskTypeChn = oList.item(i).childNodes(j).text;&#13;&#10;			if (attrname=="actionId") actionId = oList.item(i).childNodes(j).text;&#13;&#10;			if (attrname=="actionName") actionName = oList.item(i).childNodes(j).text;&#13;&#10;			//if (attrname=="useActionConArg") useActionConArg = oList.item(i).childNodes(j).text;&#13;&#10;			&#13;&#10;			&#13;&#10;			//if (attrname=="taskDesc") taskDesc = oList.item(i).childNodes(j).text;&#13;&#10;			if (attrname=="role") roleids = oList.item(i).childNodes(j).text;&#13;&#10;			if (attrname=="rolename") rolenames = oList.item(i).childNodes(j).text;&#13;&#10;			if (attrname=="copy_role") copyRoleids = oList.item(i).childNodes(j).text;&#13;&#10;			if (attrname=="copy_rolename") copyRolenames = oList.item(i).childNodes(j).text;&#13;&#10;			&#13;&#10;			if (attrname=="group") groupids = oList.item(i).childNodes(j).text;&#13;&#10;			if (attrname=="groupname") groupnames = oList.item(i).childNodes(j).text;&#13;&#10;			if (attrname=="copy_group") copyGroupids = oList.item(i).childNodes(j).text;&#13;&#10;			if (attrname=="copy_groupname") copyGroupnames = oList.item(i).childNodes(j).text;&#13;&#10;&#13;&#10;			if (attrname=="user") userids = oList.item(i).childNodes(j).text;&#13;&#10;			if (attrname =="username") usernames = oList.item(i).childNodes(j).text;&#13;&#10;			if (attrname=="copy_user") copyUserids = oList.item(i).childNodes(j).text;&#13;&#10;			if (attrname =="copy_username") copyUsernames = oList.item(i).childNodes(j).text;&#13;&#10;&#13;&#10;		}&#13;&#10;&#13;&#10;		var varclasses="";//增加2012-4-1&#13;&#10;		var oClass = oList.item(i).getElementsByTagName("var-class");&#13;&#10;		for (var ix=0;ix<oClass.length;ix++){&#13;&#10;			varclasses += oClass.item(ix).xml;	&#13;&#10;		}		&#13;&#10;		&#13;&#10;		var reminder ="";//增加reminder2012-5-14&#13;&#10;		var oReminder = oList.item(i).getElementsByTagName("reminder");&#13;&#10;		&#13;&#10;		for (var ii=0;ii<oReminder.length;ii++){&#13;&#10;			reminder  += oReminder.item(ii).xml;	&#13;&#10;		} &#13;&#10;		//strXml +="<td>" + taskDesc + "</td>";//任务描述&#13;&#10;		strXml +="<td>" + taskType + "</td>";&#13;&#10;		strXml +="<td>" + taskTypeChn + "</td>";&#13;&#10;		strXml +="<td>" + actionId + "</td>";&#13;&#10;		strXml +="<td>" + actionName + "</td>";&#13;&#10;		//strXml +="<td>" + useActionConArg + "</td>";&#13;&#10;		&#13;&#10;		strXml +="<td>" + userids + "</td>";&#13;&#10;		strXml +="<td>" + usernames + "</td>";&#13;&#10;		strXml +="<td>" + copyUserids + "</td>";&#13;&#10;		strXml +="<td>" + copyUsernames + "</td>";&#13;&#10;				&#13;&#10;		strXml +="<td>" + roleids + "</td>";&#13;&#10;		strXml +="<td>" + rolenames + "</td>";&#13