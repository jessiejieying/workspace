ton5,"../../fceform/images/ef_run_button_ok.gif");&#13;&#10;	//SetButtonImage(FCButton6,"../../fceform/images/ef_run_button_close.gif");&#13;&#10;	PropWinOnload();&#13;&#10;&#13;&#10;	dropdownlist9.FieldNameList="task_type_chn,task_type";&#13;&#10;	&#13;&#10;	//属性页&#13;&#10;	var objOpener = window.dialogArguments;&#13;&#10;	var objNodePropertys = objOpener.G_NodePropertys;&#13;&#10;	var objAction  = new NodePropertys();&#13;&#10;	if( objNodePropertys ){&#13;&#10;	&#13;&#10;		$id("txtID").value = objNodePropertys.ID;&#13;&#10;		$id("txtName").value = objNodePropertys.Name;&#13;&#10;		if (objNodePropertys.Compensate)&#13;&#10;			$id("txtComClass").value = objNodePropertys.Compensate;&#13;&#10;		&#13;&#10;		//增加动作的属性设置 &#13;&#10;		$id("txtActionID").value = objNodePropertys.ActionID;&#13;&#10;		if (objNodePropertys.ActionProperty){&#13;&#10;			objAction = objNodePropertys.ActionProperty;&#13;&#10;	&#13;&#10;			//$id("txtID").value = objNodePropertys.ID;&#13;&#10;			//$id("txtName").value = objNodePropertys.Name;			&#13;&#10;			if (objAction.View)&#13;&#10;				$id("txtView").value = objAction.View;&#13;&#10;				&#13;&#10;			if (objAction.Remark)&#13;&#10;				$id("txtRemark").value = objAction.Remark;&#13;&#10;								&#13;&#10;			if (objAction.Auto && objAction.Auto=="true")&#13;&#10;				SetCheckBoxValue(chkAuto,"true");	&#13;&#10;				&#13;&#10;			if (objAction.Finish && objAction.Finish =="true")&#13;&#10;				SetCheckBoxValue(chkFinish,"true");	&#13;&#10;						&#13;&#10;			if (objAction.UserActionType && !IsSpace(objAction.UserActionType))&#13;&#10;				$id("cboActionType").value = objAction.UserActionType;&#13;&#10;												&#13;&#10;			if (objAction.bs_table)&#13;&#10;				$id("bsTableName").value = objAction.bs_table;&#13;&#10;			if (objAction.bs_id_field)	&#13;&#10;				$id("bsIdFieldName").value = objAction.bs_id_field;&#13;&#10;			if (objAction.bs_desc_field)&#13;&#10;				$id("bsDescFieldName").value = objAction.bs_desc_field ;&#13;&#10;			//业务表单关联动态分支实例ID&#13;&#