function select() {
	var start = $("#text21").val();
	var end = $("#text23").val();
	var car="start_date="+start+"&end_date="+end+"&status=1";
	var url=project+"/admin/carInfoManager/selectCarInfos.action";
	$.post(url,car,function(data){
		for(var i in data){
			$("#sel_dropdownlist4").append($("<option>").text(data[i].licenseNum).val(data[i].id));
		}
	});
	var driver="start_date="+start+"&end_date="+end;
	url=project+"/admin/driverInfoManager/selectDriverInfos.action";
		$.post(url,driver,function(data){
			for(var i in data){
			$("#sel_dropdownlist3").append($("<option>").text(data[i].name).val(data[i].id));
			}
			});
	
}
function close(){
	window.top.closeWin("CarOrder");
}
function add(){
	//加载预选车辆
var carId=$("#text38").val();
	if(carId=="0"){
		$("#text25").val("全部");
	}
	else{
	var car="id="+carId;
	var url=project+"/admin/carInfoManager/selectCarInfos.action";
	$.post(url,car,function(data){
		for(var i in data.data){
			$("#text25").val(data.data[i].licenseNum);
		}
		
	});
		}
	//加载分配的车辆
	var car2=$("#car").val();
//	alert("car"+car2);
	if(car2.length===0){
		$("#text42").val("未选择");
	}
	else{
	var carid="id="+car2;
	var url2=project+"/admin/carInfoManager/selectCarInfos.action";
	$.post(url2,carid,function(data){
		//console.log(data);
		for(var i in data.data){
			
			//alert(data.data[i].licenseNum);
			$("#text42").val(data.data[i].licenseNum);
		}
		
	});
		}
	//加载司机
	var driverId=$("#driver").val();
	//alert("driver"+driverId);
	if(driverId.length===0){
		$("#text40").val("未选择");
	}
	else{
	var driver="id="+driverId;
	var url3=project+"/admin/driverInfoManager/selectDriverInfos.action";
	$.post(url3,driver,function(data){
		for(var i in data){
			//alert(data[i].name);
			$("#text40").val(data[i].name);
		}
		
	});
		}


}
