		   break;
		}
		case 'secretariat_director':   //秘书科处长
		{				
		   choiceUsers();
		   break;
		}
		case 'main_audit':   //主办处室审核
		{				
		   choiceUsers();
		   break;
		}
		case 'operator_deal':   //经办人办理
		{				
		   choiceNoUser();
		   break;
		}	
		case 'office_audit':   //厅领导审核
		{				
		   choiceUsers();
		   break;
		}
		case 'province_audit':   //省领导审核
