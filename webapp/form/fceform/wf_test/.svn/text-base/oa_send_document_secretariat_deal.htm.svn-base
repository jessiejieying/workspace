function showAttach(){
    $('#attachArea').slideDown();
}
function hideAttach(){
    $('#attachArea').slideUp();
}

function parseOpinion (word,name,time){
    time=time.substr(0,4)+'年'+time.substr(5,2)+'月'+time.substr(8,2)+'日';
	var html='<li><div class="opinion_word">'+word+'</div><div class="opinion_time">'+time+'</div><div class="opinion_man">'+name+'</div></li>';
	return html;
}
function finishForm(){
   
  var data=window.data_dataset2;
  var length=data.dataCount();
  for(var i=0;i<length;i++){
      
      var val=data.item(data.idByIndex(i));
	  var html=parseOpinion(val.ADVICE_CONTENT,val.CREATE_BY,val.CREATE_DTTM);
     if(val.ADVICE_TYPE=='核稿'){
            $('#opinion_3').append(html);
    }else if(val.ADVICE_TYPE=='审核'){
             $('#opinion_2').append(html);
    }else if(val.ADVICE_TYPE=='签发'){
            $('#opinion_1').append(html);
    }
    
  }
  afterLoad();
}

var wfName =  QueryURL.GetValue('wfName');
var wfDesc =  QueryURL.GetValue('wfDesc');
wfDesc=unescape(wfDesc);
var wfVersion =  QueryURL.GetValue('wfVersion');
var wfId = QueryURL.GetValue('wfId');
var dynamicNodeId =  QueryURL.GetValue('dynamic_node_id');
var dynamicNodeType =  QueryURL.GetValue('dynamic_node_type');
var splitCount = 0;
var oParam=[];           //分支的参数   [ [参数名,中文名] ]
var parentId =  undToSp(QueryURL.GetValue('dynamicInstanceId'));
function addParentCondition() {
	var str = "";

	if (parentId!='') {
		str = " and i.parent_id=" +parentId;
	}
	return str;
}

function afterLoad() {
	var DS2=getDs('DsMain');
	var Ds2ItemCount=DS2.dataCount();
	splitCount=Ds2ItemCount;
	$('#txtCount').val(splitCount);
	//查找 动态分支节点中定义的 参数信息

	var sKey = "?operate=get_dynamic_node_param&wf_name=" + wfName + "&wf_version=" + wfVersion;
	sKey += "&dynamic_node_id=" + dynamicNodeId;
	sKey += "&dynamic_node_type=" + dynamicNodeType;
	$.ajax({
		type       :"POST",
		url        :project+"/servlet/WorkflowPortal"+sKey,
		processData:false,
		data       :'<root></root>',
		contentType:'utf-8',
		success    :function (msg) {
			var oDom=newXml(msg);
			var bResult = $(oDom).find('n').text();
			if (bResult == "false") {
				$.dialog.alert("获取动态节点参数列表发生错误：" + $(oDom).find('message').text(),5);
				return;
			}else{
				$(oDom).find('record').each(function(){
					oParam.push([$(this).find('td:eq(0)').text(),$(this).find('td:eq(1)').text()]);
				});
			}

		},
		error      :function (msg) {
			$.dialog.tips('访问后台失败！')
		}
	});
	var grid=getGrid('dssubGrid');
	var DS=getDs('dssub');
	var nValue='';
	var nText='';
	grid.attachEvent("onRowDblClicked", function(rId,cInd){
		var nowEditCellId = grid.getColumnId(cInd);
		if (nowEditCellId == 'param_show_user') {
			createWindow({
				text    :'选择用户',
				url     :project + '/form/fceform/common/run.html?djsn=selUser&djtype=eden_form',
				id      :'commonSelUser',
				width   :'670',
				height  :'400',
				//parent_window:window,
				callBack:function (val, text) {
					nText = text.toString();
					nValue = val.toString();
					var nowEditItem=DS.item(rId);
					nowEditItem.param_show_user=nText;
					nowEditItem.param_show_role='';
					nowEditItem.param_show_group='';
					nowEditItem.param_value=nValue;
					DS.update(rId, nowEditItem);
				}
			})
		} else if (nowEditCellId == 'param_show_role') {
			createWindow({
				text    :'选择角色',
				url     :project + '/form/fceform/common/run.html?djsn=selRole&djtype=eden_form',
				id      :'commonSelRole',
				width   :'670',
				height  :'400',
				//parent_window:window,
				callBack:function (val, text) {
					nText = text.toString();
					nValue = val.toString();
					var nowEditItem=DS.item(rId);
					nowEditItem.param_show_user='';
					nowEditItem.param_show_role=nText;
					nowEditItem.param_show_group='';
					nowEditItem.param_value=nValue;
					DS.update(rId, nowEditItem);

				}
			})
		} else if (nowEditCellId == 'param_show_group') {
			createWindow({
				text    :'选择群组',
				url     :project + '/form/fceform/common/run.html?djsn=selGroup&djtype=eden_form',
				id      :'commonSelGroup',
				width   :'670',
				height  :'400',
				//parent_window:window,
				callBack:function (val, text) {
					nText = text.toString();
					nValue = val.toString();
					var nowEditItem=DS.item(rId);
					nowEditItem.param_show_user='';
					nowEditItem.param_show_role='';
					nowEditItem.param_show_group=nText;
					nowEditItem.param_value=nValue;
					DS.update(rId, nowEditItem);
				}
			})
		}


	});
}

function uf_add_row() {
	var DS=getDs('dssub');
	var dynamicId = getMaxIntNo('BBB');
	for (var i = 0; i < oParam.length; i++) {
		var param = oParam[i];       // [参数名,描述]
		var item={
			dynamic_instance_id:dynamicId,
			param_desc:param[1],
			param_name:param[0]
		};
		gridAddRow('dssubGrid',item);

	}
	var DS2=getDs('DsMain');
	var parent_id='';
	if (parentId!='') {
		parent_id = parentId;
	}else{
		parent_id='-1';
	}
	var item2={
		dynamic_instance_id:dynamicId,
		wf_name:wfName,
		wf_version:wfVersion,
		wf_id:wfId,
		dynamic_node_id:dynamicNodeId,
		dynamic_node_type:dynamicNodeType,
		create_user_id:GetSessionOne('userid'),
		create_date:GetDate(),
		parent_id:parent_id
	}
	gridAddRow('DsMainGrid',item2);
	splitCount++;
	$('#txtCount').val(splitCount);
}
function uf_delete_row() {
	var DS=getDs('dssub');
	var DS2=getDs('DsMain');
	var grid=getGrid('dssubGrid');
	var nowId=grid.getSelectedRowId();
	if(nowId== null){
		$.dialog.tips('请选中一行！');
		return;
	}


	var nowItem=DS.item(nowId);
	var dynamicId=nowItem.dynamic_instance_id;
	var DsItemCount=DS.dataCount();
	var deleteIds=[];
	for (var i = 0; i < DsItemCount; i++) {
		var nowId=DS.idByIndex (i);
		var nowItem=DS.item(nowId);
		if(nowItem.dynamic_instance_id==dynamicId){
			deleteIds.push(nowId);
		}

	}
	for (var i = 0; i < deleteIds.length; i++) {
		var thisId = deleteIds[i];
		var delItem=DS.item(thisId);
		_gridDelId.push(['dssub',thisId,delItem]);
		DS.remove(thisId);
	}
	var Ds2ItemCount=DS2.dataCount();
	var deleteIds2=[];
	for (var i = 0; i < Ds2ItemCount; i++) {
		var nowId=DS2.idByIndex (i);
		var nowItem=DS2.item(nowId);
		if(nowItem.dynamic_instance_id==dynamicId){
			deleteIds2.push(nowId);
		}

	}
	for (var i = 0; i < deleteIds2.length; i++) {
		var thisId = deleteIds2[i];
		var delItem=DS2.item(thisId);
		_gridDelId.push(['DsMain',thisId,delItem]);
		DS2.remove(thisId);

	}


	splitCount-=1;
	$('#txtCount').val(splitCount);

}
