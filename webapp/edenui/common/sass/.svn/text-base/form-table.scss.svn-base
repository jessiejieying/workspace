// 弹出意见样式表
.form-input-selection {
  margin: 16px 27px 27px;
  .form-input-selection-tab-ul {
    text-align: center;
    li {
      display: inline-block;
      width: 117px;
      height: 29px;
      line-height: 27px;
      text-align: center;
      background: url("../images/document-form/layer-ul-li.jpg") repeat-x left top;
      border: 1px solid #c9c9c9;
      border-bottom: 1px solid #478add;
      font-size: 14px;
      color: #666;
      cursor: pointer;
      border-top-right-radius: 4px;
      border-top-left-radius: 4px;
      position: relative;

    }
    .active {
      background: #609fee;
      border: 2px solid #478add;
      border-bottom: 1px solid #609fee;
      color: $white;
      font-weight: bold;
      top: 1px;
    }
  }
  .form-input-selection-content {
    border: 2px solid #478add;
    height: 388px;
    position: relative;
    @include radius();
    .selection-content {
      display: none;
      .handwritting-input {
        text-align: center;
        img {
        }
      }
      .clear-content {
        width: 48px;
        height: 70px;
        line-height: 70px;
        background-color: #cce1fc;
        cursor: pointer;
        border-bottom-left-radius: 5px;
        border-bottom-right-radius: 5px;
        font-size: 14px;
        color: #478add;
        text-align: center;
        position: absolute;
        top: 0px;
        right: 10px;
        &:hover {
          background-color: #abcaf0;
        }
      }
      .mixed-clear-content {
        top: 130px;
      }
      .computer-input {
        height: 248px;
        width: 750px;
        border: 0;
        background-color: transparent;
        font-size: 16px;
        color: #2f2f2f;
        padding: 20px 0 0 20px;
      }
      .mixed-content-result {
        font-size: 12px;
      }
      .mixed-input {
        border-bottom: 1px solid #609fee;
        width: 842px;
        .mixed-input-left, .mixed-input-right {

          float: left;
          width: 49%;
          .mixed-input-header {
            height: 30px;
            line-height: 30px;
            color: #478add;
            font-weight: bold;
            font-size: 12px;
            padding-left: 14px;
          }
          .mixed-body {
            height: 98px;
            overflow: auto;
            .mixed-body-ul {
              li {
                background-color: #e3e3e3;
                height: 24px;
                line-height: 24px;
                margin-top: 1px;
                color: #2f2f2f;
                cursor: pointer;
                padding-left: 16px;
                font-size: 12px;
                position: relative;
                white-space: nowrap;
                overflow: hidden;
                text-overflow: ellipsis;
                padding-right: 40px;
                .li-close {
                  position: absolute;
                  right: 20px;
                  top: 50%;
                  margin-top: -12px;
                  color: #999;
                  font-size: 16px;
                  display: none;
                }
                &:hover .li-close {
                  display: block;
                }
              }
              .on {
                background-color: #77aff5;
                color: $white;
              }
            }
          }
        }
        .mixed-input-left {
          border-right: 2px solid #609fee;
        }
        .mixed-input-right {
          float: left;

        }
      }
    }
    .on {
      display: block !important;
    }
  }
  .form-footer {
    text-align: center;
    margin-top: 10px;
    label {
      cursor: pointer;
      font-size: 12px;
      color: #2f2f2f;
      margin-right: 12px;
      input {
        vertical-align: -3px;
        margin-right: 7px;
      }
    }

    .form-input-selection-sure {
      display: inline-block;
      width: 150px;
      height: 36px;
      line-height: 34px;
      border: 1px solid #4786d5;
      text-align: center;
      color: $white;
      font-size: 14px;
      font-weight: bold;
      cursor: pointer;
      background: url("../images/document-form/layer-sure.jpg") repeat-x #5995df;
      @include radius();
      box-shadow: 0 0 5px #c5c5c5;
    }
  }
}

