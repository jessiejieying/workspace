$color-red: #a42629;
//配置主色调
$color-red01: #f14d41;
$color-yellow: #fcd32d;
$font-color: #ff7f86;

//配置table表格样式
$table-border-color: #ef655d;
$table-td-boder-color: #ef655d;
$table-content-color: #f2655d;

$white: #fff;
$black: #000;

$main-font-family:SimSun,Microsoft YaHei, Verdana, sans-serif, serif;
$main-font-family-other:SimHei,Microsoft YaHei, Verdana, sans-serif, serif;


@mixin boxShadow() {

  box-shadow: 3px 3px 3px #bababa;
}

@mixin radiusBot() {
  border-bottom-left-radius: 5px;
  border-bottom-right-radius: 5px;
}

@mixin radius() {
  border-radius: 5px;
}

@mixin radiusTop() {
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
}

//清除浮动

@mixin clearfix() {
  &:after {
    clear: both;
    content: '.';
    display: block;
    height: 0;
    line-height: 0;
    overflow: hidden;
  }
  *height: 1%;
}

.form-bg {
  height: 100%;
  background: #bde4f5 url("../images/document-form/form-bg.jpg") repeat-x;
  position: relative;
}

.form-list-pop {
  height: 100%;
  font-family: $main-font-family;
  position: relative;
  .layer-close {
    position: absolute;
    top: 0px;
    right: 0px;
    cursor: pointer;
  }
  .form-header-top {

    text-align: center;
    width: 100%;
    .form-header-top-ul {
      li {
        position: relative;
        width: 81px;
        height: 50px;
        line-height: 50px;
        display: inline-block;
        margin: 0 -4px;
        background: url("../images/document-form/form-header-li-bg.jpg") repeat-x;
        text-align: center;
        cursor: pointer;
        font-size: 14px;
        font-weight: bold;
        border-bottom: 1px solid #234d6b;
        box-shadow: 0 1px 0 #3d87ba;
        &:after {
          content: " ";
          width: 1px;
          height: 50px;
          position: absolute;
          top: 0px;
          right: 1px;
          background: url("../images/document-form/form-header-li-line.jpg") no-repeat right -1px;
        }
        a {
          color: #fff;
          text-decoration: none;
        }
      }
      .li-first {
        border-left: 1px solid #3582b9;
        border-bottom-left-radius: 10px;
      }
      .li-last {

        border-right: 1px solid #3582b9;
        border-bottom-right-radius: 10px;
        &:after {
          width: 0px;
        }
      }
    }
  }

  .form-body {
    width: 796px;
    margin: 0 auto;
    margin-top: 57px;
    border-top: 2px solid #4179a1;
    box-shadow: 0 0 5px #5b8fb1;
    position: relative;

    .form-body-header {
      height: 28px;
      width: 100%;
      text-align: center;
      position: absolute;
      left: 0px;
      top: -27px;
      .form-tab-ul {
        li {
          display: inline-block;
          font-weight: bold;
          font-size: 14px;
          color: #666;
          height: 28px;
          line-height: 28px;
          width: 92px;
          text-align: center;
          cursor: pointer;
          margin: 0 -2px;
          background: url("../images/document-form/form-body-tab-li-bg.jpg") repeat-x top center;
          border-bottom-left-radius: 2px;
          border-bottom-right-radius: 2px;
          border-top-right-radius: 5px;
          border-top-left-radius: 5px;
        }
        .active {
          color: #000;
          background: #fff;
        }
      }
    }
    .form-body-tab {
      background-color: $white;
      .form-body-tab-content {
        display: none;
        height: 100%;
        overflow: auto;
      }
      .active {
        display: block;
      }
    }
  }
}

