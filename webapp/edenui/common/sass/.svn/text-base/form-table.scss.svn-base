.datalist-link::after {
  position: absolute;
  display: block;
  width: 8px;
  height: 8px;
  content: '';
  right: 6px;
  top: 50%;
  margin-top: -4px;
  border-style: solid;
  border-width: 1px 1px 0 0;
  -ms-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
  -o-transform: rotate(45deg);
  transform: rotate(45deg);
}

.m-list {
  li {
    > a {
      &::after {
        position: absolute;
        display: block;
        width: 8px;
        height: 8px;
        content: '';
        right: 6px;
        top: 50%;
        margin-top: -4px;
        border-style: solid;
        border-width: 1px 1px 0 0;
        -ms-transform: rotate(45deg);
        -moz-transform: rotate(45deg);
        -webkit-transform: rotate(45deg);
        -o-transform: rotate(45deg);
        transform: rotate(45deg);
