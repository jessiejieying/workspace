<!DOCTYPE html>
<html style="overflow: hidden; height: 100%;">
<head>
	<!--页面共用样式-->
	<#include "/aufw/page/include/head.html">
</head>

<body style="height: 100%;">

	<!--  body 的style  应属性加上 height:100%;   是为了计算页面高度   -->

	<div id="tree_wrap" style="width: 100%; height: 100%; overflow: hidden; display: none;">
		<div id="tree_search" class="tree_search"></div>
		<div id="tree" style="width: 100%; padding-top: 3px;"></div>
	</div>

	<!-- tree_wrap 这个 div 用于放置 左侧树和搜索树的工具栏 -->

	<div id="grid_wrap" style="width: 100%; height: 100%; overflow: hidden; display: none;">
		<div id="grid_toolbar" class="ed-toolbar">
				<@f.operateRight operate="/admin/orderInfoManager/insert.action">
				<a btn_id="add" class="btn btn-info">预订车辆</a>
				</@f.operateRight> 
				<@f.operateRight operate="/admin/orderInfoManager/deleteOrderInfo.action">
				<a btn_id="delete" class="btn btn-warning">删除</a>
				</@f.operateRight>
			  <select name="" id="search">
						<option value="useBy">用车人</option>

				</select>

				<input id="search_text" class="search_text" value="请输入关键字" name="" type="text">
				<input class="btn btn-info" name="" onClick="SimpleSearch()" value="查 找" type="button">
				<a id="advanced_search_btn" class="btn">高级查找</a>

		</div>
		<div id="advanced_search_bar" class="ed-advanced-search-bar">
			<table cellpadding="0" cellspacing="0" border="0" align="center">
				<tr>
					<td><label for="search_useBy">用车人：</label></td>
					<td><input id="search_useBy" name="search_useBy" value=""
						type="text"></td>
					<td><label for="search_licenseNum">车牌号：</label></td>
					<td><input id="search_licenseNum" name="search_licenseNum"
						value="" type="text"></td>
					<td><label for="search_useDep">用车部门：</label></td>
					<td><input id="search_useDep" name="search_useDep" value=""
						type="text"></td>
					<td><label for="search_approve">类型：</label></td>
					<td><select id="search_approve" name="search_approve"
						value="0" type="text">
							<option value="">全部</option> <#list approveStatus as dataDict>
							<option value="${dataDict.dictValue}">${dataDict.displayName}</option>
							</#list> </td>
					<td><input class="btn btn-info" name="advancedSearch" onClick="advancedSearch()" value="查 找" type="button"></td>
				</tr>
			</table>
		</div>

		<div id="grid" style="width: 100%;"></div>
		<div class="ed-grid-page">
	    	<div id="grid_page"></div>
	      </div>


	</div>

</body>

<#include "/aufw/page/include/script.html">
<script src="${base}/form/fceform/js/MicrosoftAjax.js"></script>
<script src="${base}/form/fceform/js/fcpub.js"></script>
<script src="${base}/form/fceform/js/fcrundj.js"></script>
<script src="${base}/form/fceform/js/tabpane.js"></script>


<script>

	
		
    var layout,treeObj,tree,treeSearch,grid,data,listShowR,isDetailR,option,sendUrl,form;
  
   
	 window.dhx_globalImgPath="${base}/aufw/resources/common/dhtmlx/imgs/";
    <!--    设置dhtmlx各组件用到的图片的的路径-->
    function init() {       //初始化 方法
        
		
		
        layout = new dhtmlXLayoutObject(document.body, "1C");     //添加一个的layout


        layout.cells('a').hideHeader();         //隐藏 cell a 的 标题 ， b c 同理

        layout.attachEvent('onResizeFinish', page_layout);
        layout.attachEvent('onPanelResizeFinish', page_layout);
        <!-- 为layout添加监听事件    -->
        <!-- onResizeFinish 为在layout大小变了之后触发 -->
        <!-- onPanelResizeFinish 为在layout中一块区域大小变了之后触发 -->
		
		

        data = new dhtmlXDataStore({
            url: "${base}/admin/orderInfoManager/selectOrderInfos.action?etc="+ new Date().getTime(),
            datatype: "json"
        });     //添加一个 为DataStore
		

        layout.cells('a').attachObject('grid_wrap');
	
        // 在 layout的 cell b上添加 ID为 grid_wrap 的div
        grid = new dhtmlXGridObject('grid');
        // 把 grid 添加到  ID为 grid的div中
        grid.setImagePath("${base}/aufw/resources/common/dhtmlx/imgs/");
        //设置 grid上 图片的路径
        grid.setHeader(" ,车牌号,用车人,用车部门,开始时间,结束时间,审批人,审批,状态,操作");
        //设置 grid每列的标题
        grid.setColumnIds("check,licenseNum,useBy,useDep,startDate,endDate,approveBy,approve,cancel,operate");
        //设置 grid每列的数据  对应 json中的哪条数据
		grid.setInitWidths("30,80,60,80,120,120,60,60,60,*");
        //设置 每列的宽度
          
		grid.setColTypes("ch,ro,ro,ro,ro,ro,ro,co,co,link");
        //设置每列的 类型

		
	
		
		
		<#list approveStatus as dataDict>                                                                         
		grid.getCombo(7).put(${dataDict.dictValue},'${dataDict.dictName}');
		</#list>
		<#list orderCancelStatus as dataDict>                                                                         
		grid.getCombo(8).put(${dataDict.dictValue},'${dataDict.dictName}');
		</#list>
		
        grid.init();
		//grid.setEditable(false);
        // 初始化grid
		//grid.setEditable(false);
        grid.enablePaging(true,15,2, 'grid_page', true);
        //添加 grid的分页
        //grid.enablePaging(fl, pageSize, pagesInGrp, parentObj, showRecInfo, recInfoParentObj)
/*          @参数: fl - true|false - 是否分页
        *   @参数: pageSize - 每页的行数
        *   @参数: pagesInGrp - 可见的页面的数量
        *   @参数: parentObj - 放置分页的 div的ID
        *   @参数: showRecInfo - true|false - enable|disable showing of additional info about paging state
        *   @参数: recInfoParentObj - ID or container which will be used for showing paging state
*/

        grid.setPagingSkin("toolbar");   //设置 分页的皮肤
		
		grid.attachEvent("onRowSelect", doOnRowSelected);
        grid.sync(data);                // grid从 DataStore中同步数据



		var formData = [
        { type:"settings", position:"label-left", labelWidth:"65", inputWidth:50 },
        {
            type:"fieldset", label:"订单信息", inputWidth:"660", offsetLeft:10,
            list:[
                {
                    type:"hidden", offsetTop:5, list:[
					{ type:"hidden", name:"id", value:"", label:"id" },
                    { type:"input", name:"useBy", value:"", label:"用车人",inputWidth:150 , required:true, validate:"NotEmpty" },
                    { type:"input", name:"totalNum", value:"", label:"总人数", inputWidth:150 },
					// { type:"newcolumn", offset:40},
					{ type:"input", name:"startDate", value:"", label:"开始时间", inputWidth:150 },
					
					{ type:"input", name:"endDate", value:"", label:"结束时间", inputWidth:150, required:true, validate:"NotEmpty" },
					{ type:"newcolumn", offset:40},
					{ type:"input", name:"dest", value:"", label:"目的地", inputWidth:150 },
					{ type:"select", name:"driverId", value:"", label:"司机", inputWidth:150 ,options:[
						         {text:"", value:""},
							<#list driverName as dataDict>                                                                         
								{text:"${dataDict.name}", value:"${dataDict.id}"}
								<#if dataDict_has_next>
									,
								</#if>
							</#list>
							]},
					//{ type:"newcolumn", offset:40},
					{ type:"select", name:"approve", value:"", label:"审批", inputWidth:150 ,options:[
						         {text:"", value:""},
							<#list approveStatus as dataDict>                                                                         
								{text:"${dataDict.displayName}", value:"${dataDict.dictValue}"}
								<#if dataDict_has_next>
									,
								</#if>
							</#list>
							]},
					{ type:"select", name:"feedback", value:"", label:"反馈", inputWidth:150 ,options:[
						         {text:"", value:""},
							<#list feedbackStatus as dataDict>                                                                         
								{text:"${dataDict.displayName}", value:"${dataDict.dictValue}"}
								<#if dataDict_has_next>
									,
								</#if>
							</#list>
							]},
					

                ]
                },{
                    type:"hidden", offsetTop:5, list:[
                     {type:"label",labelWidth:270},
                     {type:"newcolumn"},
                     {type:"button",value:"确定",name:"send"}
                ]
                }
                ]
        }
        ];
             

        page_layout();      // 调用page_layout方法   ,方法的声明在下面

    }
	
	function order(){
		window.top.createWindow({
            'text':'车辆预定',
            'id':'CarOrder',
            'url':'${base}/form/fceform/common/run.html?djsn=oa_car_order_apply_for_wf&djtype=eden_form&time='+new Date(),
            'width':'600',
            'height':'500',
            parent_ifr:window
		});

	}
	
	
	function SimpleSearch(){
		data.clearAll();
		var clu = $("#search").val();
		var value = $("#search_text").val()=='请输入关键字'?'':$("#search_text").val();

		var url = "${base}/admin/orderInfoManager/selectOrderInfos.action?orderInfo."+clu+"="+ window.encodeURI(value)+"&etc="+ new Date().getTime();
		window.data.data.url = url
		data.load(url);
	}

    function  advancedSearch(){
		data.clearAll();
	
		var useBy = $("#search_useBy").val()==''?'':$("#search_useBy").val();
		var useDep = $("#search_useDep").val()==''?'':$("#search_useDep").val();
		var licenseNum = $("#search_licenseNum").val()==''?'':$("#search_licenseNum").val();
		var approve = $("#search_approve").val()==''?'':$("#search_approve").val();


		var url = "${base}/admin/orderInfoManager/selectOrderInfos.action?orderInfo.useBy="+ window.encodeURI(useBy)+"&orderInfo.useDep="+ window.encodeURI(useDep)+"&orderInfo.approve="+approve+"&licenseNum="+ window.encodeURI(licenseNum)+"&etc="+ new Date().getTime();
		window.data.data.url = url
		data.load(url);
    }

	
	function doOnRowSelected(id) {
	
		if(!window._add_new_child){

			updateFormBtn({
            "dataStore":data,
            "grid":grid,
            "rowId":id,
            "forms":[form]
        });
        window._add_new_child=false;
		}else{
			return false;
		}
	}
   

	
	function view(id){
		grid.selectRowById(id);
		var item=data.item(id);
		window.top.createWindow({
				text    :'查看订单信息',
				'url':	'${base}/form/fceform/common/run.html?djsn=oa_car_order_detail_for_shceduler&djtype=eden_form&id='+id,
				id      :'CarOrder',
				width   :'600',
				height  :'500'			
		})
	}
	
	function view_wf(id){
		grid.selectRowById(id);
		var wf_id = data.item(id).wfId;
		var cancel = data.item(id).cancel;
		if(cancel=='0'){
			$.dialog.tips('该流程已经删除');

		}
		else{
			showWfHistory(wf_id);
		}
	}
	

    function toolbar_func(id){           // grid上面 的那些按钮的 功能
        if (id == 'add') {
			order();
        }
		
		
		
        if (id == 'edit') {
        	//eventWin('修改事件','${base}/form/fceform/common/run.html?djsn=eventEdit&djtype=itsm&wf_id=678')
        }
		
		
		
        if (id == 'delete') {
		        var row_id = grid.getCheckedRows(0);
		
	        if (row_id == '') {
	            $.dialog.tips('请至少选择一条数据！');
	            return;
	        }
			var idArr=new Array();
	        idArr = row_id.split(',');
			
	        if (row_id.length == 0) return;
	        $.dialog.confirm('你确定要删除这些数据吗？', function () {
          
		   var wf_idArr = new Array();
            var nameArr=new Array();
            for (var i = 0; i < idArr.length; i++) {

                wf_idArr.push(data.item(idArr[i]).wfId);
                
           }
			

            $.post("${base}/admin/orderInfoManager/deleteOrderInfo.action", {"idArr":row_id,"wf_idArr":wf_idArr.join(",")},function(result){
            	if(result.result="success"){         
                   // for (var i = 0; i < idArr.length; i++) {           
                   //    data.remove(idArr[i]);
                    //}
					SimpleSearch();
            	}
                $.dialog.tips(result.tips);
				//SimpleSearch();
            });
        });
           
        }
    }
    function page_layout() {                 // page_layout为控制页面布局的方法 ，  用于自适应窗口的大小
        var grid_wrap_height = $('#grid_wrap').height();  //得到 ID为  grid_wrap 的div的高度
        $('#grid').height(grid_wrap_height - 43 -35);    //设置  ID为  grid 的div的高度
        if(grid)    grid.setSizes();                   // 如果grid 存在，则 让 grid 适应下窗口的大小

    }
	
	
	
	
	function cancel(id) {
		grid.selectRowById(id);
		var wf_id = data.item(id).wfId;
		var cancel = data.item(id).cancel;
		var feedback = data.item(id).feedback;

		if(cancel=='0'){
			$.dialog.tips('该记录已经取消');
		}
		else{

			$.dialog.confirm('确定取消该记录？', function () {
       
				$.post("${base}/admin/orderInfoManager/deleteOrderInfoWf.action", {"wf_idArr":wf_id}, function(datatest){
					if(datatest.result=="success"){
					SimpleSearch();	 
					}
			
					$.dialog.tips(datatest.tips);
				});
			});
		}
	}

	function d(id) {
		grid.selectRowById(id);
		var wf_id = data.item(id).wfId;
		var cancel = data.item(id).cancel;
		var feedback = data.item(id).feedback;

		$.dialog.confirm('你确定要删除这条数据？', function () {
       
			$.post("${base}/admin/orderInfoManager/deleteOrderInfo.action", {"wf_idArr":wf_id}, function(datatest){
				if(datatest.result=="success"){
				
					data.remove(id);	 
				}
            $.dialog.tips(datatest.tips);	
			});
		});
		
	}
	
	function e(id) {      //编辑 grid中的一行数据
		grid.selectRowById(id);
		window._now_grid_edit_id=id;      //把当前正在编辑的这条数据的ID 保存起来，用于  form保存后，将结果同时保存到左侧树中；
		// form.setItemFocus("userName");
		option="update";
		form.enableItem("send");
    
	}

	
    $(function () {
        setTimeout(function(){init();}, 0);                        //在 页面加载好后，触发init方法；
		

		//$('#grid_toolbar_ul li').last().addClass('last');   //为grid上面的一排按钮中的最后一个li添加一个为‘last’的class
        $('#grid_toolbar a').click(function(){       //那些按钮 点击之后 触发 toolbar_func 方法
            var id=$(this).attr('btn_id');
            toolbar_func(id);
        });
		
		$("#search_text").focus(function () {
            if ($(this).val() == this.defaultValue) {
                $(this).val('');
            }
        }).blur(function () {
            if ($(this).val() == '') {
                $(this).val(this.defaultValue);
            }
        });


		//高级搜索展开
		//searchBarCommon('advanced_search_bar',30,'advanced_search_btn','grid_toolbar','grid');
		searchToggle('advanced_search_btn','advanced_search_bar');
    })
</script>
</html>