	<link rel="stylesheet" type="text/css"
	href="${base}/aufw/resources/common/artDialog/skins/default.css">
	<style>
	#myForm:after{
	clear: both;
    content: ".";
    display: block;
    height: 0;
    overflow: hidden;
    visibility: hidden;
	}
	#myForm{  zoom:1;}
	</style>
	
</head>

<body style="height:100%;+overflow-x:hidden;background:#fff;">
	<div id="myForm" style="float:none;">
	</div>
	<div style="height:20px;overflow:hidden;clear:both;"></div>
</body>
<script src="${base}/aufw/resources/common/js/jquery.js"></script>
