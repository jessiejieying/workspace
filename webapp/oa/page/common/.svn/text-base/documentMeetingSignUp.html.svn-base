                    <span class="float-left h26 font-bold ">已签收单位</span>
                    <button ms-if="@hasSignList.length>0" ms-click="@instructionToFb()" class="button button-main margin-left float-lfet">附件汇总</button>
                    <span class="float-right h26">{{@hasSignCount}}家</span>
                </div>
                <table class="border-top-blue margin-top">
                    <tr>
                        <!-- 右侧 -->
                        <td class="ver-top">
                            <div class="js-grid-father grid-father unit-table">
                                <table class="table-fixed-header bsgrid-header" style="position: fixed;left:43px;top:347px;width: 797px;">
                                    <tr>
                                        <th width="40">
                                            <input ms-duplex-checked="@hasSignListCheckAll" data-duplex-changed="@hasSignListCheckAllChange" class="standard-checkbox" type="checkbox" />
                                        </th>
