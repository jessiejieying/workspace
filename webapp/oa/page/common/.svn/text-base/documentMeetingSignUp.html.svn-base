<script src="${base}/edenui/common/js/jq_plugins/bsgrid/bsgrid.all.min.js"></script>
<script src="${base}/edenui/common/js/jq_plugins/bsgrid/grid.extend.min.js"></script>
<script src="${base}/edenui/common/js/underscore.js"></script>
<script src="${base}/edenui/common/js/avalon/avalon2.js"></script>
<script>
var $advice = getUrlParam('advice');
var $noAccount = '';

var undistinguishData = [];
var isShowNumber = false;

var stepParam = 'step100';

//全部的单位数据
var allUnitData = [];

var messageData = "";

var mailMessageData = {
    'sendRange': [],
    'telphones': [],
    'content': '',
    //单次为1，多次为2
    'taskType': 1,
    //多次的时间间隔
    'cycleHour': 1,
    'messageType': 2
}
var mailMessageDataDirect = {
    'sendRange': [],
    'telphones': [],
    'content': '',
    'messageType': 2
}

//发送短信内容
var mailMessage = {
    "accounts": "",
    'telphones': "",
    'content': ""
}

//参数配置表
avalon.config({
    debug: false
});
var model = avalon.define({
    $id: "wrap",
    //群组列表
    groupList:[],
    //群组下拉选项
    groupOption:[],
    groupSelect:'',
    //群组搜索
    searchGroup:'',

    //处室列表
    officeList:[],
    //群主下拉选项
    officeOption:[],
    officeSelect:'',
    //处室搜索
    searchOffice:'',
    //选中的人
    choiceList:[],
    //删除右侧人员
    choiceListDel:function(obj){

        model.choiceList.forEach(function(value,index){
            if(value.account==obj.account){
                model.choiceList.splice(index,1);
            }
        });

        //清除左侧的选中状态

        model.groupList.forEach(function(value){
            if(value.account==obj.account){
                value.select=false;

            }
        });

            model.officeList.forEach(function(value){
                if(value.account==obj.account){
                    value.select=false;
                }
            });

    },

    //1是群组，2是处室
    choiceAll:function(type){

        var choiceListData=model.choiceList.map(function(value){
            return value.account;
        });

        if(type=="1"){
            model.groupList.forEach(function(value){
                if(choiceListData.indexOf(value.account)<0){
                    model.choiceList.push({
                        "account":value.account,
                        "name":value.name,
                        "seq":value.seq
                    });

                }
                value.select=true;
            });
        }
        else if(type=="2"){
            model.officeList.forEach(function(value){
                if(choiceListData.indexOf(value.account)<0){

                    model.choiceList.push({
                        "account":value.account,
                        "name":value.name,
                        "seq":value.seq
                    });

                }
                value.select=true;
            });
        }

    },
    // obj：传递对象，idnex：索引值，num：1、群组，2、处室
    choiceClick:function(obj,index,num){

        var isPush=true;
        model.choiceList.forEach(function(value){

            //是否已经存在列表中
            if(value.account==obj.account){
                isPush=false;
            }
        });

        if(isPush){

            model.choiceList.push({
                "account":obj.account,
                "name":obj.name,
                "seq":obj.seq
            });

            model.choiceList.sort(function(a,b){
                return a.seq- b.seq
            });


            if(num=="1"){
                model.groupList[index].select=true;
            }
            else if(num=="2"){
                model.officeList[index].select=true;
            }

        }

    },
    clearAll:function(){

        model.choiceList.clear();

        model.officeList.forEach(function(value){
            value.select=false;
        });
        model.groupList.forEach(function(value){
            value.select=false;
        });

    },
    fileDatas: [],
    indexShow: 0,
    //未识别单位
    undistinguishData: [],
    //是否存在报名表
    formExist: false,
    meetingEntryList: {
        //参会人员名单
        dataJoin: [],
        //请假人员名单
        dataLeave: [],
        //已上报单位
        hasFeedbackOrg: '',
        hasSignCount: 0,
        //未上报单位
        toFeedbackOrg: '',
        toSignCount: 0
    },
    //搜索区域弹出层
    searchHtmlData: [],
    searchHtmlShow: false,
    searchHtmlDataClick: function(obj) {

        var testText = $("#distinguish_text").val();
        var dataPush = testText.replace(/，/g, ",").replace(/、/g, ",").replace(/;/g, ",").replace(/；/g, ",").split(",");

        dataPush[dataPush.length - 1] = obj.content;
        $("#distinguish_text").val(dataPush.join(",") + ",").focus();

        model.searchHtmlShow = false;

    },
    distinguishChange: function() {
        var testText = $("#distinguish_text").val();
        var dataPush = testText.replace(/，/g, ",").replace(/、/g, ",").replace(/;/g, ",").replace(/；/g, ",").split(",");

        var $val = dataPush[dataPush.length - 1];
        var searchData = [];
        //暂存输入单位
        window.top.toExchangeHtml = $("#distinguish_text").val();

        model.searchHtmlData.length = 0;
        if ($val.length) {
            allUnitData.forEach(function(value) {

                if ((value.name).indexOf($val) === 0) {

                    searchData.push({
                        'content': value.name
                    })
                }
            });

            model.searchHtmlData = searchData;
            if (searchData.length) {
                model.searchHtmlShow = true;
            }

        }
    },
    sendExist: true,
    feedBackExist: true,
    isFinish:true,
    //附件列表全选项
    fileListCheck: false,
    fileListCheckChange: function(e) {
        var checked = e.target.checked;

        if (checked) {
            model.fileDatas.forEach(function(value) {
                value.check = true;
