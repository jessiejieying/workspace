                </div>
                <div id="choiceBotBody" class="layer-choice-content-body clearfix " style="margin-top:11px;height:100px;border: 0;">
                    <div class="body-choice-left bdblue">
                        <ul>
                            <li ms-for="(index,el) in @groupList" ms-attr="{'data-id':el.id,'data-account':el.account}"  ms-click="@choiceClick(el,index,1)" ms-visible="el.isShow" ms-class="{selected:el.select}">{{el.name}}</li>
                        </ul>

                    </div>

                    <div class="body-choice-right-only body-choice-right">
