        </div>
        <select id="left_obj" class="chooser_man_list" size="1" multiple>
        </select>
    </div>
    <div class="add_remove_area">
        <input onclick="swap_obj($('#left_obj option:selected'),$('#right_obj'), 'add')" class="chooser_add_btn" name=""
               type="button">
        <input onclick="swap_obj($('#right_obj option:selected'),$('#left_obj'), 'reduce')" class="chooser_remove_btn"
